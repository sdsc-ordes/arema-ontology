@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/taxonomy/> .
@prefix brick: <https://w3id.org/brick#> .
@prefix ifc: <https://standards.buildingsmart.org/IFC/DEV/IFC4/ADD2_TC1/OWL#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .


# Top-level classes (formerly Concept Schemes)
ex:BuildingMaterial rdfs:label "Building Materials"@en , "Baustoffe"@de , "Matériaux de construction"@fr , "Materiali da costruzione"@it .
ex:Building rdfs:label "Buildings"@en , "Gebäude"@de , "Bâtiments"@fr , "Edifici"@it .
ex:NaturalResource rdfs:label "Natural Resources"@en , "Natürliche Ressourcen"@de , "Ressources naturelles"@fr , "Risorse naturali"@it .
ex:Professional rdfs:label "Professionals"@en , "Fachleute"@de , "Professionnels"@fr , "Professionisti"@it .
ex:TechnicalConstruction rdfs:label "Technical Constructions"@en , "Technische Konstruktionen"@de , "Constructions techniques"@fr , "Costruzioni tecniche"@it ;
    rdfs:comment "Aspects of construction related to the physical realization of buildings, encompassing structural, thermal, acoustic, and service-related elements."@en .

# Building Materials Taxonomy
ex:BMType rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:label "Type"@en , "Typ"@de , "Type"@fr , "Tipo"@it .

ex:Massive rdfs:subClassOf ex:BMType ;
    rdfs:label "Massive"@en , "Massivlehmbau"@de , "Massif"@fr , "Massiccio"@it .

ex:Bricks rdfs:subClassOf ex:BMType ;
    rdfs:label "Bricks"@en , "Baustein"@de , "Briques"@fr , "Mattoni"@it .

ex:Panels rdfs:subClassOf ex:BMType ;
    rdfs:label "Panels, Boards"@en , "Platten"@de , "Plaques"@fr , "Pannelli"@it .

ex:Bulk rdfs:subClassOf ex:BMType ;
    rdfs:label "Bulk"@en , "Schüttgut"@de , "Vrac"@fr , "Massa"@it .

ex:Mortar rdfs:subClassOf ex:BMType ;
    rdfs:label "Mortar, Plaster"@en , "Mörtel, Putz"@de , "Mortier, Crépis"@fr , "Malta, Intonaco"@it .

# Buildings Taxonomy
ex:BuildingStatus rdfs:subClassOf ex:Building ;
    rdfs:label "Intervention Type"@en , "Eingriffstyp"@de , "Type d'intervention"@fr , "Tipo di intervento"@it .

ex:NewBuilding rdfs:subClassOf ex:BuildingStatus ;
    rdfs:label "New"@en , "Neu"@de , "Nouveau"@fr , "Nuovo"@it .

ex:RenovationBuilding rdfs:subClassOf ex:BuildingStatus ;
    rdfs:label "Renovation"@en , "Renovierung"@de , "Rénovation"@fr , "Ristrutturazione"@it .

ex:BuildingType rdfs:subClassOf ex:Building ;
    rdfs:label "Type"@en , "Typ"@de , "Type"@fr , "Tipo"@it .

ex:ApartmentBuilding rdfs:subClassOf ex:BuildingType ;
    rdfs:label "Apartment Building"@en , "Mehrfamilienhaus"@de , "Immeuble d'habitation"@fr , "Edificio residenziale"@it .

ex:SingleFamilyHouse rdfs:subClassOf ex:BuildingType ;
    rdfs:label "Single Family House"@en , "Einfamilienhaus"@de , "Maison individuelle"@fr , "Casa unifamiliare"@it .

ex:OfficeBuilding rdfs:subClassOf ex:BuildingType ;
    rdfs:label "Office Building"@en , "Bürogebäude"@de , "Immeuble de bureaux"@fr , "Edificio per uffici"@it .

ex:PublicBuilding rdfs:subClassOf ex:BuildingType ;
    rdfs:label "Public Building"@en , "Öffentliches Gebäude"@de , "Bâtiment public"@fr , "Edificio pubblico"@it .

ex:SimpleStructure rdfs:subClassOf ex:BuildingType ;
    rdfs:label "Simple Structure"@en , "Einfache Struktur"@de , "Structure simple"@fr , "Struttura semplice"@it .

# Natural Resources Taxonomy
ex:NRType rdfs:subClassOf ex:NaturalResource ;
    rdfs:label "Type"@en , "Typ"@de , "Type"@fr , "Tipo"@it .

ex:Earth rdfs:subClassOf ex:NRType ;
    rdfs:label "Earth"@en , "Erde"@de , "Terre"@fr , "Terra"@it .

ex:Rock rdfs:subClassOf ex:NRType ;
    rdfs:label "Rock"@en , "Gestein"@de , "Roche"@fr , "Rocce"@it .

ex:Straw rdfs:subClassOf ex:NRType ;
    rdfs:label "Straw"@en , "Stroh"@de , "Paille"@fr , "Paglia"@it .

ex:Hemp rdfs:subClassOf ex:NRType ;
    rdfs:label "Hemp"@en , "Hanf"@de , "Chanvre"@fr , "Canapa"@it .

ex:Wood rdfs:subClassOf ex:NRType ;
    rdfs:label "Wood"@en , "Holz"@de , "Bois"@fr , "Legno"@it .

ex:OtherFibers rdfs:subClassOf ex:NRType ;
    rdfs:label "Other Fibers"@en , "Andere Fasern"@de , "Autres fibres"@fr , "Altre fibre"@it .

# Professionals Taxonomy
ex:ProfType rdfs:subClassOf ex:Professional ;
    rdfs:label "Type"@en , "Typ"@de , "Type"@fr , "Tipo"@it .

ex:Architect rdfs:subClassOf ex:ProfType ;
    rdfs:label "Architect"@en , "Architekt"@de , "Architecte"@fr , "Architetto"@it .

ex:CivilEngineer rdfs:subClassOf ex:ProfType ;
    rdfs:label "Civil engineer"@en , "Bauingenieur"@de , "Ingénieur civil"@fr , "Ingegnere civile"@it .

ex:Craftsman rdfs:subClassOf ex:ProfType ;
    rdfs:label "Craftsman"@en , "Handwerker"@de , "Artisan"@fr , "Artigiano"@it .

ex:MaterialProducer rdfs:subClassOf ex:ProfType ;
    rdfs:label "Material Producer"@en , "Materialhersteller"@de , "Producteur de matériaux"@fr , "Produttore di materiali"@it .

ex:BuildingPhysicsProfessional rdfs:subClassOf ex:ProfType ;
    rdfs:label "Building Physics"@en , "Bauphysik"@de , "Physique du bâtiment"@fr , "Fisica dell'edificio"@it .

ex:AssociationProfessional rdfs:subClassOf ex:ProfType ;
    rdfs:label "Association"@en , "Verband"@de , "Association"@fr , "Associazione"@it .

ex:ConstructionFirmProfessional rdfs:subClassOf ex:ProfType ;
    rdfs:label "Construction Firm"@en , "Bauunternehmen"@de , "Entreprise de construction"@fr , "Impresa di costruzioni"@it .

ex:ResearcherProfessional rdfs:subClassOf ex:ProfType ;
    rdfs:label "Researcher"@en , "Forscher"@de , "Chercheur"@fr , "Ricercatore"@it .

ex:ResellerProfessional rdfs:subClassOf ex:ProfType ;
    rdfs:label "Reseller"@en , "Wiederverkäufer"@de , "Revendeur"@fr , "Rivenditore"@it .

ex:ProfessionalOther rdfs:subClassOf ex:ProfType ;
    rdfs:label "Other"@en , "Andere"@de , "Autre"@fr , "Altro"@it .

# Technical Constructions Taxonomy
ex:FireProtection rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Fire Protection"@en , "Brandschutz"@de , "Protection incendie"@fr , "Protezione antincendio"@it ;
    rdfs:comment "Measures and design elements intended to prevent, contain, and mitigate the effects of fire within a building."@en .

ex:ThermalInsulation rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Thermal Insulation"@en , "Wärmeschutz"@de , "Isolation thermique"@fr , "Isolamento termico"@it ;
    rdfs:comment "Techniques and materials used to reduce the transfer of heat between the inside and outside of a building."@en .

ex:MoistureProtection rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Moisture Protection"@en , "Feuchteschutz"@de , "Protection contre l'humidité"@fr , "Protezione dall'umidità"@it ;
    rdfs:comment "Strategies and components used to prevent moisture infiltration and control humidity in building structures."@en .

ex:SoundInsulation rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Sound Insulation"@en , "Schallschutz"@de , "Isolation acoustique"@fr , "Isolamento acustico"@it ;
    rdfs:comment "Methods and materials designed to reduce the transmission of sound between and within building spaces."@en .

ex:LoadBearingStructure rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Load-bearing Structure"@en , "Tragkonstruktion"@de , "Structure porteuse"@fr , "Struttura portante"@it ;
    rdfs:comment "The system of structural elements that support and transfer the weight and forces of a building to the ground."@en .

ex:LightweightConstruction rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Lightweight Construction"@en , "Leichtbau"@de , "Construction légère"@fr , "Costruzione leggera"@it ;
    rdfs:comment "Construction techniques emphasizing minimal material usage and weight while maintaining structural integrity."@en .

ex:BuildingServices rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Building Services"@en , "Technische Gebäudeausrüstung"@de , "Services du bâtiment"@fr , "Impianti tecnici dell'edificio"@it ;
    rdfs:comment "Technical systems installed in buildings, such as heating, ventilation, plumbing, and electrical infrastructure."@en .

ex:BuildingEnvelope rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Building Envelope"@en , "Gebäudehülle"@de , "Enveloppe du bâtiment"@fr , "Involucro edilizio"@it ;
    rdfs:comment "The physical separator between the interior and exterior of a building, including walls, roofs, and windows."@en .

ex:Prefabrication rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Prefabrication"@en , "Vorproduktion"@de , "Préfabrication"@fr , "Prefabbricazione"@it ;
    rdfs:comment "The process of manufacturing building components off-site for later assembly on-site."@en .

ex:RenovationTechnicalConstruction rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Renovation"@en , "Sanierung"@de , "Rénovation"@fr , "Ristrutturazione"@it ;
    rdfs:comment "The process of improving, modernizing, or restoring an existing building."@en .

ex:Deconstruction rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Deconstruction"@en , "Rückbau"@de , "Déconstruction"@fr , "Decostruzione"@it ;
    rdfs:comment "The systematic dismantling of a building with the goal of preserving materials for reuse or recycling."@en .

ex:TechnicalConstructionOther rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:label "Other"@en , "Andere"@de , "Autre"@fr , "Altro"@it ;
    rdfs:comment "Technical construction topics not covered by the predefined categories."@en .

# Building Materials
ex:BuildingMaterial
    owl:equivalentClass brick:Material , ifc:IfcMaterial .

ex:Bricks
    rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:subClassOf ifc:IfcMaterial .

ex:Concrete
    rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:subClassOf ifc:IfcMaterial .

ex:Glass
    rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:subClassOf ifc:IfcMaterial .

ex:Steel
    rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:subClassOf ifc:IfcMaterial .

ex:Wood
    rdfs:subClassOf ex:BuildingMaterial ;
    rdfs:subClassOf ifc:IfcMaterial .

# Building Types
ex:Building
    owl:equivalentClass brick:Building ;
    owl:equivalentClass ifc:IfcBuilding .

ex:ApartmentBuilding
    rdfs:subClassOf ex:Building ;
    rdfs:subClassOf ifc:IfcBuilding .

ex:OfficeBuilding
    rdfs:subClassOf ex:Building ;
    rdfs:subClassOf ifc:IfcBuilding .

ex:SingleFamilyHouse
    rdfs:subClassOf ex:Building ;
    rdfs:subClassOf ifc:IfcBuilding .

# Technical Constructions
ex:TechnicalConstruction
    rdfs:subClassOf brick:Equipment ;
    rdfs:subClassOf ifc:IfcElement .

ex:LoadBearingStructure
    rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:subClassOf ifc:IfcStructuralElement .

ex:ThermalInsulation
    rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:subClassOf ifc:IfcMaterialLayer .

ex:SoundInsulation
    rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:subClassOf ifc:IfcMaterialLayer .

ex:FireProtection
    rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:subClassOf ifc:IfcFireSuppressionTerminal .

ex:MoistureProtection
    rdfs:subClassOf ex:TechnicalConstruction ;
    rdfs:subClassOf ifc:IfcCovering .

# Professionals
ex:Professional
    rdfs:subClassOf ifc:IfcActor .

ex:Architect
    rdfs:subClassOf ex:Professional .

ex:CivilEngineer
    rdfs:subClassOf ex:Professional .

ex:ConstructionWorker
    rdfs:subClassOf ex:Professional .

ex:BuildingInspector
    rdfs:subClassOf ex:Professional .

